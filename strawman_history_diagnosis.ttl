@prefix : <http://purl.org/ejp-rd/cde/v020/example-rdf/> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ordo: <http://www.orpha.net/ORDO/> .
@prefix loinc: <http://purl.bioontology.org/ontology/LNC/> .

:person_ a sio:SIO_000498 ;  # person
    sio:SIO_000228 :role_1, :role_2, :role_3;  # has role
    sio:SIO_000217 :quality_3 .  # has quality

:role_1 a obo:OBI_0000093;  # patient
    sio:SIO_000356 :process_1 .  # is realized in

:role_2 a obo:OBI_0000093; # patient
    sio:SIO_000356 :process_2 .   # is realized in

:role_3 a obo:OBI_0000093; # patient
    sio:SIO_000356 :process_3 .   # is realized in

:process_1 a sio:SIO_001001 ;  # medical diagnosis (process)
  sio:SIO_000229 :output_1;  # has output
  sio:SIO_000680  "2017-10-18T13:00:00"^^xsd:dateTime .  # has start time
  
:process_2 a obo:NCIT_C17747 ; # disease progression process
  sio:SIO_000628 :output_1 ;  # refers to diagnostic opinion
  sio:SIO_000008 :symptom_onset .  # has attribute
  
:process_3 a sio:SIO_000006 ;  # age at diagnosis recording process
  sio:SIO_000229 :output_3;  # has output
  sio:SIO_000628 :output_1 ;  # refers to diagnostic opinion
  sio:SIO_000680  "2017-10-19T13:00:00"^^xsd:dateTime .    # has start time

:symptom_onset a obo:NCIT_C124353 ; # symptom onset attribute
    sio:SIO_000628 :onset_type . # refers to

:onset_type a loinc:LP91394-4.  # at birth
  

:output_1 a sio:SIO_000015, sio:SIO_001003; # info content entity  & Diagnosis
  sio:SIO_000628 :disease_ ;  # refers to
  sio:SIO_000300 "some disease" . # diagnostic opinions are defined as having some literal value
  


:disease_ a ordo:Orphanet_377793.

:quality_3 a obo:NCIT_C156420, sio:SIO_000005;  # age at diagnosis; quality
    sio:SIO_000216 :output_3 ;  # has measurement value
    sio:SIO_000628 :output_1 .  # refers to [diagnosis node]

:output_3 a sio:SIO_000070;  # measurement value
  sio:SIO_000300 0;   # has value 0
  sio:SIO_000221 :unit_1 .  # has unit 

:unit_1 a obo:UO_0000036 .  # year
